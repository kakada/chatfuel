= render "shared/sb-admin/page_heading", title: t("sidebar.provincial_usages")

.card.shadow.mb-4
  .card-body
    = render "download"
    = render "dashboard/filter_options"

    #table.table-responsive
      %table.table.table-hover
        %thead
          = render 'header_row'
        %tbody
          - @provincial_usages.each_with_index do |provincial, index|
            %tr
              %td= index + 1
              - provincial.data.each do |data|
                %td= data
