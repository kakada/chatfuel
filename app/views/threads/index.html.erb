<div>
  <table>
    <tr>
      <th>mid</th>
      <th>progress</th>
      <th>-</th>
    </tr>
    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.mid %></td>
        <td><%= lead.progress %></td>
        <td><%= link_to 'show', thread_path(lead) %></td>
      </tr>
    <% end %>
  </table>
</div>
